<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aeho.demo.dao.KeepDao">

  <select id="listKeep" resultType="com.aeho.demo.vo.KeepVo">
  	select * from keep where m_id = #{m_id}
  </select>
  
  <insert id="insertKeep">
  	insert into keep(k_no, m_id, b_no)
  	values(seq_keep.nextval, #{m_id}, #{b_no})
  </insert>
  
  <delete id="deleteKeep">
  	delete keep where k_no = #{k_no}
  </delete>
</mapper>